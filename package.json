{
  "name": "nextjs",
  "version": "4.0.0",
  "private": true,
  "dependencies": {
    "@formatjs/intl-relativetimeformat": "^7.2.10",
    "@material-ui/core": "latest",
    "accepts": "^1.3.7",
    "clsx": "latest",
    "cookies": "^0.8.0",
    "express": "^4.17.1",
    "glob": "^7.1.6",
    "http-proxy": "^1.18.1",
    "intl": "^1.2.5",
    "jsdom": "^16.4.0",
    "jsom": "^1.0.0",
    "next": "latest",
    "prop-types": "latest",
    "react": "latest",
    "react-dom": "latest",
    "react-final-form": "^6.5.1",
    "react-helmet": "^6.1.0",
    "react-intl": "^5.8.6"
  },
  "scripts": {
    "dev-next": "next",
    "dev": "node server.js",
    "build": "next build",
    "start": "NODE_ENV=production node server.js",
    "post-update": "echo \"codesandbox preview only, need an update\" && yarn upgrade --latest",
    "extract:i18n": "formatjs extract '{pages,components}/*.{js,ts,tsx,jsx}' --format simple --id-interpolation-pattern '[sha512:contenthash:base64:6]' --out-file lang/en.json ",
    "compile:i18n": "formatjs compile-folder --ast --format simple lang/ compiled-lang/"
  },
  "devDependencies": {
    "@formatjs/cli": "^2.13.2",
    "@types/node": "^14.11.8",
    "babel-plugin-react-intl": "^8.2.7",
    "typescript": "^4.0.3"
  }
}
